<%= render :partial => 'form' %>
<ul>
<% @locations.each do |location|%>
  <li><%= location.place %></li>
  <li><%= location.long %>, <%= location.lat %></li>
  <li> Radius: <%= location.radius %></li>
  <li><%= link_to('Edit', edit_location_path(location)) %> | <%= link_to('Delete', location, :data =>{:confirm=>'Are you sure you want to do delete this location?',:method => :delete }) %>
  </li>
<% end %>
</ul>